{
  "compilerOptions": {
    "target": "es2016",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@app/health-check": [
        "libs/health-check/src"
      ],
      "@app/health-check/*": [
        "libs/health-check/src/*"
      ]
    }
  }
}